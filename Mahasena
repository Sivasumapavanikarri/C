#include <stdio.h>

int main() {
    int N;
    scanf("%d", &N);

    int even = 0, odd = 0;
    for (int i = 0; i < N; i++) {
        int A;
        scanf("%d", &A);
        if (A % 2 == 0)
            even++;
        else
            odd++;
    }

    if (even > odd)
        printf("READY FOR BATTLE\n");
    else
        printf("NOT READY\n");

    return 0;
}
